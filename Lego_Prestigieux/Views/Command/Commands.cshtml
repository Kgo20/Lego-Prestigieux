@model List<Lego_Prestigieux.Models.CommandModel>

<div class="container-fluid">
    <div class="row px-xl-5">
        <div class="col-lg-10 table-responsive mb-5">
            <table class="table table-light table-borderless table-hover text-center mb-0">
                <thead class="thead-dark">
                    <tr>
                        <th class="align-middle center align-items-center">N° de commande</th>
                        <th class="align-middle center align-items-center">Date de commande</th>
                        <th class="align-middle center align-items-center">Livraison prévue le</th>
                        <th class="align-middle center align-items-center">Montant de la commande</th>
                        <th class="align-middle center align-items-center">Nombre de produits</th>
                        <th class="align-middle center align-items-center">Status</th>
                        <th class="align-middle center align-items-center">Annuler</th>
                        <th class="align-middle center align-items-center">Reçu</th>

                    </tr>
                </thead>
                <tbody class="align-middle">
                    @foreach (var item in Model)
                    {
                        <vc:command id="item.Id"></vc:command>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>